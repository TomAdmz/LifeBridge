<div class="page-header">
  <h1>Mentee Matches</h1>
</div>
<div class="panel-body">
    <table class="table table-striped table-hover">
      <thead>
        <tr>

            <th>UserName</th>

            <th>Profession</th>

            <th></th>

        </tr>
      </thead>

      <tbody>

        {{#each rows}}

            <tr>

                <td>{{this.username}}

                <td>{{this.prof}}

                <td><a href="http://localhost:50000/" class="btn btn-default btn-lg">Request Mentee</a>

            </tr>

        {{/each}}

      </tbody>

      </table>
</div>

<div class="jumpotron">
	<p>
		<a href="http://localhost:50000/" class="btn btn-default btn-lg">Return Home</a>
	</p>

</div>